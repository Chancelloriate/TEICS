<p id="notice"><%= notice %></p>

<h1>Listing Environments</h1>

<table>
  <thead>
    <tr>
      <th>Environmentid</th>
      <th>Environmentname</th>
      <th>Environmentdescription</th>
      <th>Createdby</th>
      <th>Createddate</th>
      <th>Lastmodifiedby</th>
      <th>Lastmodifieddate</th>
      <th>Isdeleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @environments.each do |environment| %>
      <tr>
        <td><%= environment.EnvironmentId %></td>
        <td><%= environment.EnvironmentName %></td>
        <td><%= environment.EnvironmentDescription %></td>
        <td><%= environment.CreatedBy %></td>
        <td><%= environment.CreatedDate %></td>
        <td><%= environment.LastModifiedBy %></td>
        <td><%= environment.LastModifiedDate %></td>
        <td><%= environment.IsDeleted %></td>
        <td><%= link_to 'Show', environment %></td>
        <td><%= link_to 'Edit', edit_environment_path(environment) %></td>
        <td><%= link_to 'Destroy', environment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Environment', new_environment_path %>
