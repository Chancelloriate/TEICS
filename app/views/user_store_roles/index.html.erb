<p id="notice"><%= notice %></p>

<h1>Listing User Store Roles</h1>

<table>
  <thead>
    <tr>
      <th>Userstoreroleid</th>
      <th>Roleid</th>
      <th>Userstoreid</th>
      <th>Createdby</th>
      <th>Createddate</th>
      <th>Lastmodifiedby</th>
      <th>Lastmodifieddate</th>
      <th>Isdeleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_store_roles.each do |user_store_role| %>
      <tr>
        <td><%= user_store_role.UserStoreRoleId %></td>
        <td><%= user_store_role.RoleId %></td>
        <td><%= user_store_role.UserStoreId %></td>
        <td><%= user_store_role.CreatedBy %></td>
        <td><%= user_store_role.CreatedDate %></td>
        <td><%= user_store_role.LastModifiedBy %></td>
        <td><%= user_store_role.LastModifiedDate %></td>
        <td><%= user_store_role.IsDeleted %></td>
        <td><%= link_to 'Show', user_store_role %></td>
        <td><%= link_to 'Edit', edit_user_store_role_path(user_store_role) %></td>
        <td><%= link_to 'Destroy', user_store_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User store role', new_user_store_role_path %>
