<p id="notice"><%= notice %></p>

<h1>Listing User Source Links</h1>

<table>
  <thead>
    <tr>
      <th>Usersourcelinkid</th>
      <th>Sourcelinkid</th>
      <th>Userstoreid</th>
      <th>Createdby</th>
      <th>Createddate</th>
      <th>Lastmodifiedby</th>
      <th>Lastmodifieddate</th>
      <th>Isdeleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_source_links.each do |user_source_link| %>
      <tr>
        <td><%= user_source_link.UserSourceLinkId %></td>
        <td><%= user_source_link.SourceLinkId %></td>
        <td><%= user_source_link.UserStoreId %></td>
        <td><%= user_source_link.CreatedBy %></td>
        <td><%= user_source_link.CreatedDate %></td>
        <td><%= user_source_link.LastModifiedBy %></td>
        <td><%= user_source_link.LastModifiedDate %></td>
        <td><%= user_source_link.IsDeleted %></td>
        <td><%= link_to 'Show', user_source_link %></td>
        <td><%= link_to 'Edit', edit_user_source_link_path(user_source_link) %></td>
        <td><%= link_to 'Destroy', user_source_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User source link', new_user_source_link_path %>
