<p id="notice"><%= notice %></p>

<h1>Listing Cert Store Links</h1>

<table>
  <thead>
    <tr>
      <th>Certstorelinkid</th>
      <th>Sourcelinkid</th>
      <th>Certstoreid</th>
      <th>Createdby</th>
      <th>Createddate</th>
      <th>Lastmodifiedby</th>
      <th>Lastmodifieddate</th>
      <th>Isdeleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cert_store_links.each do |cert_store_link| %>
      <tr>
        <td><%= cert_store_link.CertStoreLinkId %></td>
        <td><%= cert_store_link.SourceLinkId %></td>
        <td><%= cert_store_link.CertStoreId %></td>
        <td><%= cert_store_link.CreatedBy %></td>
        <td><%= cert_store_link.CreatedDate %></td>
        <td><%= cert_store_link.LastModifiedBy %></td>
        <td><%= cert_store_link.LastModifiedDate %></td>
        <td><%= cert_store_link.IsDeleted %></td>
        <td><%= link_to 'Show', cert_store_link %></td>
        <td><%= link_to 'Edit', edit_cert_store_link_path(cert_store_link) %></td>
        <td><%= link_to 'Destroy', cert_store_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cert store link', new_cert_store_link_path %>
