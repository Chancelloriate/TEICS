<p id="notice"><%= notice %></p>

<h1>Listing Member Groups</h1>

<table>
  <thead>
    <tr>
      <th>Membergroupid</th>
      <th>Networkuserid</th>
      <th>Usergroupid</th>
      <th>Createdby</th>
      <th>Createddate</th>
      <th>Lastmodifiedby</th>
      <th>Lastmodifieddate</th>
      <th>Isdeleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @member_groups.each do |member_group| %>
      <tr>
        <td><%= member_group.MemberGroupId %></td>
        <td><%= member_group.NetworkUserId %></td>
        <td><%= member_group.UserGroupId %></td>
        <td><%= member_group.CreatedBy %></td>
        <td><%= member_group.CreatedDate %></td>
        <td><%= member_group.LastModifiedBy %></td>
        <td><%= member_group.LastModifiedDate %></td>
        <td><%= member_group.IsDeleted %></td>
        <td><%= link_to 'Show', member_group %></td>
        <td><%= link_to 'Edit', edit_member_group_path(member_group) %></td>
        <td><%= link_to 'Destroy', member_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member group', new_member_group_path %>
