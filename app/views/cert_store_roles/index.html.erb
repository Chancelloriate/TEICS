<p id="notice"><%= notice %></p>

<h1>Listing Cert Store Roles</h1>

<table>
  <thead>
    <tr>
      <th>Certstoreroleid</th>
      <th>Roleid</th>
      <th>Certstoreid</th>
      <th>Createdby</th>
      <th>Createddate</th>
      <th>Lastmodifiedby</th>
      <th>Lastmodifieddate</th>
      <th>Isdeleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cert_store_roles.each do |cert_store_role| %>
      <tr>
        <td><%= cert_store_role.CertStoreRoleId %></td>
        <td><%= cert_store_role.RoleId %></td>
        <td><%= cert_store_role.CertStoreId %></td>
        <td><%= cert_store_role.CreatedBy %></td>
        <td><%= cert_store_role.CreatedDate %></td>
        <td><%= cert_store_role.LastModifiedBy %></td>
        <td><%= cert_store_role.LastModifiedDate %></td>
        <td><%= cert_store_role.IsDeleted %></td>
        <td><%= link_to 'Show', cert_store_role %></td>
        <td><%= link_to 'Edit', edit_cert_store_role_path(cert_store_role) %></td>
        <td><%= link_to 'Destroy', cert_store_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cert store role', new_cert_store_role_path %>
