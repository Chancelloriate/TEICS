<p id="notice"><%= notice %></p>

<h1>Listing User Groups</h1>

<table>
  <thead>
    <tr>
      <th>Usergroupid</th>
      <th>Usergroupname</th>
      <th>Createdby</th>
      <th>Createddate</th>
      <th>Lastmodifiedby</th>
      <th>Lastmodifieddate</th>
      <th>Isdeleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_groups.each do |user_group| %>
      <tr>
        <td><%= user_group.UserGroupId %></td>
        <td><%= user_group.UserGroupName %></td>
        <td><%= user_group.CreatedBy %></td>
        <td><%= user_group.CreatedDate %></td>
        <td><%= user_group.LastModifiedBy %></td>
        <td><%= user_group.LastModifiedDate %></td>
        <td><%= user_group.IsDeleted %></td>
        <td><%= link_to 'Show', user_group %></td>
        <td><%= link_to 'Edit', edit_user_group_path(user_group) %></td>
        <td><%= link_to 'Destroy', user_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User group', new_user_group_path %>
