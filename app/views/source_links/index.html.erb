<p id="notice"><%= notice %></p>

<h1>Listing Source Links</h1>

<table>
  <thead>
    <tr>
      <th>Sourcelinkid</th>
      <th>Hyperlink</th>
      <th>Createdby</th>
      <th>Createddate</th>
      <th>Lastmodifiedby</th>
      <th>Lastmodifieddate</th>
      <th>Isdeleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @source_links.each do |source_link| %>
      <tr>
        <td><%= source_link.SourceLinkId %></td>
        <td><%= source_link.Hyperlink %></td>
        <td><%= source_link.CreatedBy %></td>
        <td><%= source_link.CreatedDate %></td>
        <td><%= source_link.LastModifiedBy %></td>
        <td><%= source_link.LastModifiedDate %></td>
        <td><%= source_link.IsDeleted %></td>
        <td><%= link_to 'Show', source_link %></td>
        <td><%= link_to 'Edit', edit_source_link_path(source_link) %></td>
        <td><%= link_to 'Destroy', source_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Source link', new_source_link_path %>
