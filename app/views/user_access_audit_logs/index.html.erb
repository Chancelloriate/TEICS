<p id="notice"><%= notice %></p>

<h1>Listing User Access Audit Logs</h1>

<table>
  <thead>
    <tr>
      <th>Auditlogid</th>
      <th>Recordid</th>
      <th>Tablename</th>
      <th>Attribute</th>
      <th>Previousvalue</th>
      <th>Currentvalue</th>
      <th>Createdby</th>
      <th>Createddate</th>
      <th>Lastmodifiedby</th>
      <th>Lastmodifieddate</th>
      <th>Isdeleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_access_audit_logs.each do |user_access_audit_log| %>
      <tr>
        <td><%= user_access_audit_log.AuditLogId %></td>
        <td><%= user_access_audit_log.RecordId %></td>
        <td><%= user_access_audit_log.TableName %></td>
        <td><%= user_access_audit_log.Attribute %></td>
        <td><%= user_access_audit_log.PreviousValue %></td>
        <td><%= user_access_audit_log.CurrentValue %></td>
        <td><%= user_access_audit_log.CreatedBy %></td>
        <td><%= user_access_audit_log.CreatedDate %></td>
        <td><%= user_access_audit_log.LastModifiedBy %></td>
        <td><%= user_access_audit_log.LastModifiedDate %></td>
        <td><%= user_access_audit_log.IsDeleted %></td>
        <td><%= link_to 'Show', user_access_audit_log %></td>
        <td><%= link_to 'Edit', edit_user_access_audit_log_path(user_access_audit_log) %></td>
        <td><%= link_to 'Destroy', user_access_audit_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User access audit log', new_user_access_audit_log_path %>
