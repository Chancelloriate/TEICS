<p id="notice"><%= notice %></p>

<h1>Listing Network Users</h1>

<table>
  <thead>
    <tr>
      <th>Networkuserid</th>
      <th>Networkfirstname</th>
      <th>Networklastname</th>
      <th>Email</th>
      <th>Networkusername</th>
      <th>Networkpassword</th>
      <th>Createdby</th>
      <th>Createddate</th>
      <th>Lastmodifiedby</th>
      <th>Lastmodifieddate</th>
      <th>Isdeleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @network_users.each do |network_user| %>
      <tr>
        <td><%= network_user.NetworkUserId %></td>
        <td><%= network_user.NetworkFirstName %></td>
        <td><%= network_user.NetworkLastName %></td>
        <td><%= network_user.Email %></td>
        <td><%= network_user.NetworkUserName %></td>
        <td><%= network_user.NetworkPassword %></td>
        <td><%= network_user.CreatedBy %></td>
        <td><%= network_user.CreatedDate %></td>
        <td><%= network_user.LastModifiedBy %></td>
        <td><%= network_user.LastModifiedDate %></td>
        <td><%= network_user.IsDeleted %></td>
        <td><%= link_to 'Show', network_user %></td>
        <td><%= link_to 'Edit', edit_network_user_path(network_user) %></td>
        <td><%= link_to 'Destroy', network_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Network user', new_network_user_path %>
