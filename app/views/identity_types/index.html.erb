<p id="notice"><%= notice %></p>

<h1>Listing Identity Types</h1>

<table>
  <thead>
    <tr>
      <th>Identitytypeid</th>
      <th>Identitytypename</th>
      <th>Identitytypedescription</th>
      <th>Createdby</th>
      <th>Createddate</th>
      <th>Lastmodifiedby</th>
      <th>Lastmodifieddate</th>
      <th>Isdeleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @identity_types.each do |identity_type| %>
      <tr>
        <td><%= identity_type.IdentityTypeId %></td>
        <td><%= identity_type.IdentityTypeName %></td>
        <td><%= identity_type.IdentityTypeDescription %></td>
        <td><%= identity_type.CreatedBy %></td>
        <td><%= identity_type.CreatedDate %></td>
        <td><%= identity_type.LastModifiedBy %></td>
        <td><%= identity_type.LastModifiedDate %></td>
        <td><%= identity_type.IsDeleted %></td>
        <td><%= link_to 'Show', identity_type %></td>
        <td><%= link_to 'Edit', edit_identity_type_path(identity_type) %></td>
        <td><%= link_to 'Destroy', identity_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Identity type', new_identity_type_path %>
